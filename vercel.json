{
  "version": 2,
  "builds": [{ "src": "public/**", "use": "@vercel/static" }],
  "routes": [
    { "src": "^/sportsedge$", "dest": "/public/sportsedge/index.html" },
    { "src": "^/sportsedge/$", "dest": "/public/sportsedge/index.html" },
    { "src": "^/sportsedge/(.*)$", "dest": "/public/sportsedge/$1" },
    { "src": "^(.*)$", "dest": "/public$1" }
  ]
}
